#ifndef TTPLAYER_EXCEPTION_H_
#define TTPLAYER_EXCEPTION_H_

#include <Exception>

class TTPlayerException : public Exception {
public:
	TTPlayerException();
	virtual ~TTPlayerException();
	TTPlayerException(const TTPlayerException &other);
};

#endif /* TTPLAYER_EXCEPTION_H_ */
