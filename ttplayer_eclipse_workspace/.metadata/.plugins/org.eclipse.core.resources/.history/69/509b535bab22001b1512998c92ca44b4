#include "tt_player_effect.h"


TTPlayerEffect::TTPlayerEffect()
{
	statelessTransferFunction = new StatelessTransferFunction();
	// TODO Auto-generated constructor stub

}

TTPlayerEffect::~TTPlayerEffect() {
	delete(statelessTransferFunction);
	// TODO Auto-generated destructor stub
}

TTPlayerEffect::TTPlayerEffect(const TTPlayerEffect &other) {

	statelessTransferFunction = new StatelessTransferFunction(other.statelessTransferFunction);

}

